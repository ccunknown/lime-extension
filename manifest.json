{
  "author": "NECTEC INO",
  "content_scripts": [
    {
      "css": [
        "static/css/all.min.css",
        "static/custom/css/extension-bootstrap.css",
        "static/custom/css/extension.css"
      ],
      "js": [
        "static/config/load-schema.js"
      ]
    }
  ],
  "description": "Live Modbus Extension (LiME) is modbus extension for Mozilla-Iot gateway.",
  "gateway_specific_settings": {
    "webthings": {
